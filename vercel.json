{
  "images": {
    "domains": [],
    "sizes": [320, 640, 768, 1024, 1280, 1536],
    "formats": ["image/avif", "image/webp"],
    "minimumCacheTTL": 86400,
    "dangerouslyAllowSVG": false
  },
  "headers": [
    {
      "source": "/img/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}